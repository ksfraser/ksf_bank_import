{
    "name": "ksfraser/fa-bank-import",
    "description": "A FrontAccounting module for bank import functionality with paired transfer processing.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Ksfraser\\": "src/Ksfraser/",
            "Ksfraser\\FaBankImport\\": "src/Ksfraser/FaBankImport/",
            "Ksfraser\\FaBankImport\\Events\\": "src/Ksfraser/FaBankImport/events/",
            "Ksfraser\\Application\\": "src/Ksfraser/Application/",
            "Ksfraser\\Application\\Models\\": "src/Ksfraser/Application/models/",
            "Ksfraser\\Application\\Interfaces\\": "src/Ksfraser/Application/interfaces/",
            "Ksfraser\\HTML\\": "src/Ksfraser/HTML/",
            "Ksfraser\\Model\\": "src/Ksfraser/Model/",
            "Ksfraser\\View\\": "src/Ksfraser/View/",
            "KsfBankImport\\Services\\": "Services/",
            "KsfBankImport\\OperationTypes\\": "OperationTypes/"
        },
        "classmap": [
            "src/Ksfraser/FaBankImport/FactoryTransactions.php",
            "src/Ksfraser/FaBankImport/controllers/BankImportController.php",
            "src/Ksfraser/FaBankImport/models/",
            "src/Ksfraser/FaBankImport/Container/SimpleContainer.php",
            "src/Ksfraser/FaBankImport/Commands/CommandDispatcher.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Ksfraser\\FaBankImport\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.3",
        "asgrim/ofxparser": "^1.2",
        "mimographix/qif-library": "^1.0",
        "nesbot/carbon": "^2.46",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "codeception/codeception": "^4.1"
    },
    "scripts": {
        "test": "phpunit",
        "test-file": "phpunit",
        "post-install-cmd": [
            "@php -r \"echo '\\n✅ Dependencies installed successfully!\\n\\n';\""
        ],
        "post-update-cmd": [
            "@php -r \"echo '\\n✅ Dependencies updated successfully!\\n\\n';\""
        ]
    },
    "config": {
        "platform": {
            "php": "7.3"
        }
    }
}
